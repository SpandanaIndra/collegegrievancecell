@model IEnumerable<Core_CollegeGrievanceCell.Models.Complaint>

@{
    Layout = "_AdminLayout";
}


<h1>Solved Complaints</h1>
<br />



<div>
    <table class="table">
        <thead>
            <tr>

                <th>
                    @Html.DisplayNameFor(model => model.ComplaintId)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Description)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.DateOfComplaint)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Status)
                </th>

            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>
                        @Html.DisplayFor(modelItem => item.ComplaintId)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Description)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.DateOfComplaint)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Status)
                    </td>


                </tr>
            }
        </tbody>
    </table>

    <br />
    <a asp-action="AdminModule" asp-Controller="Admin">Back</a>

</div>
